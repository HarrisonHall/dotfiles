	HostName hachha.dev
	User root
	Port 22
	IdentityFile ~/.ssh/default

Host home-server
	HostName 192.168.1.160
	User hachha
	Port 22
	IdentityFile ~/.ssh/default

Host github.com
	IdentityFile ~/.ssh/default

